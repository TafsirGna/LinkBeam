{
  "manifest_version": 3,
  "name": "LinkBeam",
  "description": "LinkBeam chrome extension ",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "app_logo_white.png"
  },
  "background": {
    "service_worker": "./service-worker.js"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": ["*://*/*"]
}
